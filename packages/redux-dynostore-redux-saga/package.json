{
  "name": "@redux-dynostore/redux-saga",
  "version": "3.0.1",
  "description": "Dynamically add Redux sagas at runtime",
  "author": "Michael Peyper",
  "contributors": [
    "Jonathan Peyper"
  ],
  "license": "BSD-3-Clause",
  "main": "lib/index.js",
  "module": "es/index.js",
  "sideEffects": false,
  "files": [
    "lib",
    "es",
    "src"
  ],
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ioof-holdings/redux-dynostore.git"
  },
  "peerDependencies": {
    "@redux-dynostore/core": "^3.0.0",
    "redux": "^3.0.0 || ^4.0.0",
    "redux-saga": "^1.0.0"
  },
  "devDependencies": {
    "@babel/core": "7.6.4",
    "@babel/plugin-proposal-object-rest-spread": "7.6.2",
    "@babel/plugin-transform-modules-commonjs": "7.6.0",
    "@babel/plugin-transform-runtime": "7.6.2",
    "@babel/preset-env": "7.6.3",
    "@babel/runtime": "7.6.3",
    "@redux-dynostore/core": "^3.0.1",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "10.0.3",
    "babel-jest": "24.9.0",
    "babel-plugin-annotate-pure-calls": "0.4.0",
    "babel-plugin-module-resolver": "3.2.0",
    "core-js": "3.3.1",
    "cross-env": "6.0.3",
    "jest": "24.9.0",
    "redux": "4.0.4",
    "redux-saga": "1.1.1",
    "rimraf": "3.0.0",
    "rollup": "1.23.1",
    "rollup-plugin-babel": "4.3.3"
  },
  "scripts": {
    "prebuild": "rimraf lib es",
    "build": "rollup -c",
    "lint": "eslint . --ext .js --ext .jsx",
    "lint:fix": "eslint . --ext .js --ext .jsx --fix",
    "test": "jest",
    "test:watch": "npm test -- --watch",
    "prepublish": "npm run build"
  },
  "jest": {
    "collectCoverage": true,
    "coverageDirectory": "./coverage/",
    "collectCoverageFrom": [
      "**/src/**"
    ],
    "testURL": "http://localhost/"
