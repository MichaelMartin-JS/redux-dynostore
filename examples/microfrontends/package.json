{
  "name": "microfrontends-example",
  "version": "1.1.0",
  "private": true,
  "scripts": {
    "bootstrap": "lerna bootstrap",
    "postinstall": "npm run bootstrap",
    "prebuild": "npm run externals:build",
    "build": "ENV=production webpack",
    "prestart": "npm run externals:build",
    "start": "webpack-dev-server",
    "start:production": "NODE_ENV=production npm start",
    "externals:build": "lerna run --parallel build && npm run externals:copy",
    "externals:copy": "rimraf externals && copyfiles -f packages/*/bundle/* externals"
  },
  "dependencies": {
    "@reach/router": "^1.1.1",
    "@redux-dynostore/core": "^1.1.0",
    "@redux-dynostore/react-redux": "^1.1.0",
    "@redux-dynostore/react-redux-subspace": "^1.1.0",
    "@redux-dynostore/redux-subspace": "^1.1.0",
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "react-loadable": "^5.4.0",
    "react-redux": "^5.0.7",
    "redux": "^3.7.2",
    "redux-devtools": "^3.4.0",
    "redux-subspace": "^2.5.0",
    "redux-thunk": "^2.2.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.1.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "babel-register": "^6.26.0",
    "copy-webpack-plugin": "^4.5.2",
    "copyfiles": "^2.0.0",
    "html-webpack-plugin": "^3.2.0",
    "lerna": "^2.10.1",
    "redux-devtools-extension": "^2.13.5",
    "rimraf": "^2.6.2",
    "webpack": "^4.15.1",
    "webpack-cli": "^3.0.8",
    "webpack-dev-server": "^3.1.4"
  }
}
